#include "main.h"
/**

*/
int print_integ(va_list args)
{
	int num = va_args(args, int);
	int count = 0;
	if (num < 0)
	{
		count += _putchar('-');
		num = -num;
	}
	if (num == 0)
	{
		count += _putchar('0');
	}
	else
	{
		int divisor = 1;
		while (num / divisor >= 10)
			divisor *= 10;
		while (divisor > 0)
		{
			count += _putchar(num / divisor) + '0');
			num %= divisor;
			divisor /= 10;
		}
	}
	return count
}
